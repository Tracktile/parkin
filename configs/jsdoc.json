{
  "opts": {
    "encoding": "utf8",
    "destination": "docs/",
    "readme": "README.md",
    "recurse": true,
    "template": "./node_modules/clean-jsdoc-theme",
    "verbose": true
  },
  "tags": {
    "allowUnknownTags": [true]
  },
  "plugins": [
    "plugins/markdown"
  ],
  "source": {
    "include": ["src"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs|src/__mocks__|src/__tests__)"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "search": true,
    "referenceTitle": "Parkin - Gherkin Parser",
    "disableSort": false,
    "collapse": true,
    "resources": {
        "github": "https://github.com/lancetipton/parkin",
        "npm": "https://www.npmjs.com/package/@ltipton/parkin"
    }
  }
}

